{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["build/**", ".next/**", "!.next/cache/**"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"//#lint": {},
		"//#lint:fix": {
			"cache": false
		}
	}
}
